openapi: 3.0.3
info:
    title: Goa API
    version: 0.0.1
servers:
    - url: https://goa.design
paths:
    /one:
        get:
            tags:
                - testService
            summary: testEndpoint1 testService
            operationId: testService#testEndpoint1
            requestBody:
                required: true
                content:
                    application/json:
                        schema:
                            $ref: '#/components/schemas/Payload1'
                        example:
                            foo: Dicta sunt officia.
            responses:
                "200":
                    description: OK response.
                    content:
                        application/json:
                            schema:
                                $ref: '#/components/schemas/Result1'
                            example:
                                bar: Voluptas sed et esse quod eligendi ut.
    /two:
        get:
            tags:
                - testService
            summary: testEndpoint2 testService
            operationId: testService#testEndpoint2
            requestBody:
                required: true
                content:
                    application/json:
                        schema:
                            $ref: '#/components/schemas/Payload2'
                        example:
                            foo: Culpa cumque repudiandae asperiores assumenda.
            responses:
                "200":
                    description: OK response.
                    content:
                        application/json:
                            schema:
                                $ref: '#/components/schemas/Result2'
                            example:
                                bar: Exercitationem quos accusamus sunt vel sed reprehenderit.
components:
    schemas:
        Payload1:
            type: object
            properties:
                foo:
                    type: string
                    example: Beatae non id consequatur.
            example:
                foo: Aut sed ducimus repudiandae sit explicabo asperiores.
        Payload2:
            type: object
            properties:
                foo:
                    type: string
                    example: Quas aut maxime aut non enim ullam.
            example:
                foo: Vitae magni repellat minus minus dolor repellat.
        Result1:
            type: object
            properties:
                bar:
                    type: string
                    example: Qui rem qui earum.
            example:
                bar: Consequatur delectus accusantium quaerat earum ratione.
        Result2:
            type: object
            properties:
                bar:
                    type: string
                    example: Nostrum et eum et labore veritatis similique.
            example:
                bar: Eum laboriosam.
tags:
    - name: testService
